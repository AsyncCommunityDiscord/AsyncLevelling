{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchPackageNames": ["gradle"],
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    },
    {
      "matchPackageNames": ["com.fasterxml.jackson.core:jackson-databind"],
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    }
  ]
}
